{
  "flutter": {
    "platforms": {
      "ios": {
        "default": {
          "projectId": "iron-split",
          "appId": "1:742367506307:ios:fb1ffaa897d5673a19d0b6",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "iron-split",
          "configurations": {
            "ios": "1:742367506307:ios:fb1ffaa897d5673a19d0b6"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": []
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.html", 
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, no-cache" 
          }
        ]
      }
    ]
  }
}
